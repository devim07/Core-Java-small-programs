class Asg
{
	public static void main(String args[])
	{
		for(int i=1; i<=9; i++)
		{
			if(i<=5)
			{
				for(int k=5; k>i;k--)
					System.out.print(" ");
				for(int j=1; j<=(2*i-1); j++)
				{
					System.out.print("*");
				}
				System.out.println();
			}
			else
			{
				for(int k=0; k<=i-6;k++)
					System.out.print(" ");
				for(int j=13-i; j>i-6; j--)
				{
					System.out.print("*");
				}
				System.out.println();
			}
		}
	}
}
