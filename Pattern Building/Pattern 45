/*
* * * * * *
 *       *
  *     *
   *   *
    * *
     *
*/

class Asg
{
	public static void main(String args[])
	{
		for(int i=6; i>=1;i--)
		{
			for(int k=6; k>i;k--)
				System.out.print(" ");
			if(i<6)
			{
				for(int j=i; j>=1; j--)
				{
					if(j==1 || j==i)
						System.out.print("* ");
					else
						System.out.print("  ");
				}
			}
			else
			{
				for(int j=i; j>=1; j--)
					System.out.print("* ");
			}
			System.out.println();
		}
	}
}
