//WAP to print tables of 1-12 using a while loop.


import java.util.*;
class Test
{
	public static void main(String[] arg)
	{
		Scanner sc=new Scanner(System.in);
		int i=1, j=1;
		while (i<=10)
		{
			j=1;
			while(j<=12)
				System.out.printf("%-2d*%-2d=%-5d",j,i,(i*j++));
			System.out.println();
			i++;
		}
	}
}

