//WAP  to print tables of 1-30 using single for loop.

import java.util.*;
class Test
{
	public static void main(String[] arg)
	{
		Scanner sc=new Scanner(System.in);
		int j=0;
		for (int i = 0; i < 12 * 10; ++i)
        		{
            			int a = i / 12 + 1;
            			int b = i % 12 + 1;
            			System.out.printf("%-2d*%-2d=%-5d", b, a, a * b);
			             if(b==12)
				            System.out.println();
       		}
		
	  }
}
