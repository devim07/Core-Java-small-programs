//WAP  to print tables of 1-12 using 2 for loops 

import java.util.*;
class Test
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		for(int i=1; i<=10; i++)
		{
			for (int j=1;j<=12; j++)
				System.out.printf("%-2d*%-2d=%-5d", j, i,(i*j));
			System.out.println();
		}
	}
}
